{
  /// WINDOWS
  "hyprland/window#icon": {
    "format": "{title}",
    "max-length": 64,
    "icon-size": 20,
    "seperate-outputs": true,
    "icon": true
  },
  "hyprland/window#title": {
    "format": "{title}",
    "max-length": 64,
    "seperate-outputs": true,
  },
  /// WORKSPACES
  // ROMAN Numerals style
  "hyprland/workspaces#roman": {
    "active-only": false,
    "format": "{icon}",
    "show-special": false,
    "on-click": "activate",
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
    "format-icons": {
      "1": "I",
      "2": "II",
      "3": "III",
      "4": "IV",
      "5": "V",
      "6": "VI",
      "7": "VII",
      "8": "VIII",
      "9": "IX",
      "10": "X",
    },
  },
  // PACMAN Style
  "hyprland/workspaces#pacman": {
    "active-only": false,
    "format": "{icon}",
    "on-click": "activate",
    "on-scroll-up": "hyprctl dispatch workspace e+1",
    "on-scroll-down": "hyprctl dispatch workspace e-1",
    "show-special": false,
    "persistent-workspaces": {
      "*": 5
