[
  {
    "module": "cpu",
    "format": " {usage}%",
    "interval": 1,
    "tooltip": true
  },
  {
    "module": "memory",
    "format": " {used:G}G",
    "interval": 3,
    "tooltip": true
  },
  {
    "module": "clock",
    "format": "{:%a %d %b - %H:%M}",
    "interval": 60
  },
  {
    "module": "pulseaudio",
    "format": "{volume}% {icon}",
    "format-muted": "Muted 🔇",
    "on-click": "pavucontrol",
    "tooltip": true,
    "format-icons": {
      "headphone": "",
      "hands-free": "",
      "headset": "",
      "phone": "",
      "portable": "",
      "car": "",
      "default": [
        "🔈",
        "🔉",
        "🔊"
      ]
    }
  },
  {
    "module": "battery",
    "format": "{capacity}% {icon}",
    "format-charging": "Charging {capacity}% {icon}",
    "format-plugged": "Plugged {capacity}% {icon}",
    "format-icons": [
      "",
      "",
      "",
      "",
      ""
    ],
    "tooltip": true,
    "interval": 60
  },
  {
    "module": "tray",
    "icon-size": 16,
    "spacing": 5
  },
  {
    "module": "idle_inhibitor",
    "format": "{icon}",
    "format-icons": {
      "activated": "",
      "deactivated": ""
    },
    "tooltip": "Click to toggle idle inhibitor"
  },
  {
    "module": "custom/launcher",
    "format": "",
    "on-click": "rofi -show drun"
  }
]
